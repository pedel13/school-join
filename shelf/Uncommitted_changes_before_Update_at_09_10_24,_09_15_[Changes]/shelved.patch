ed = true;\n            //console.log(\"Angekommen\")\n        }\n        else {\n            confirmPassword.addEventListener(\"keyup\", ()=> {\n                signUpBtn.classList.remove(\"disabledButton\");\n                signUpBtn.disabled = false;\n            })\n        }\n    })\n}\n\n\n
